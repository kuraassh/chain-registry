{
    "$schema": "../chain.schema.json",
    "chain_name": "galaxy-1",
    "status": "live",
    "network_type": "mainnet",
    "pretty_name": "Galaxy Network",
    "chain_id": "galaxy-1",
    "bech32_prefix": "galaxy",
    "daemon_name": "galaxyd",
    "node_home": "$HOME/.galaxyd",
    "genesis": {
        "genesis_url": "https://media.githubusercontent.com/media/galaxies-labs/networks/main/galaxy-1/genesis.json"
    },
    "key_algos": [
        "secp256k1",
        "ed25519"
    ],
    "slip44": 118,
    "fees": {
        "fee_tokens": [
            {
                "denom": "uglx",
                "fixed_min_gas_price": 0.025
            }
        ]
    },
    "codebase": {
        "git_repo": "https://github.com/galaxies-labs/galaxy",
        "recommended_version": "v1.6.0",
        "compatible_versions": [
            "v1.6.0"
        ],
        "binaries": {
            "linux/amd64": "https://github.com/galaxies-labs/galaxy/releases/download/v1.0.0/galaxy_v1.0.0_linux_amd64",
            "linux/arm64": "https://github.com/galaxies-labs/galaxy/releases/download/v1.0.0/galaxy_v1.0.0_linux_amd64",
            "darwin/amd64": "https://github.com/galaxies-labs/galaxy/releases/download/v1.0.0/galaxy_v1.0.0_darwin_amd64"
        }
    },
    "peers": {
        "seeds": [
        ]
    },
    "apis": {
        "rpc": [
            {
                "address": "https://galaxy.postcapitalist.io",
                "provider": "POSTCAPITALIST"
            }
        ],
        "rest": [
        ]
    },
    "explorers": [
        {
            "kind": "POSTCAPITALIST",
            "url": "https://explorer.postcapitalist.io/Galaxy",
            "tx_page": "https://explorer.postcapitalist.io/Galaxy/tx/${txHash}"
        }
    ]
}
